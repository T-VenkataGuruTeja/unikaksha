#angular velocity

from math import *
v1=int(input())
r1=int(input())
v2=int(input())
r2=int(input())
n=int(input())
x=[]
y=[]
x.append(cos(v1*n*(3.14/180))*r1)
y.append(sin(v1*n*(3.14/180))*r1)
x.append(cos(v2*n*(3.14/180))*r2)
y.append(sin(v2*n*(3.14/180))*r2)
X=abs(x[0]-x[1])
Y=abs(y[0]-y[1])
if r1 == r2:
    print("Invalid Input")
else:
    print("%.2f"%(sqrt(X**2 + Y**2)))

#annual book fair

def prize(N,k):
    l=[]
    for i in range(N):
        temp = int(input())
        if temp < 1000 and temp > 0:
            l.append(temp)
    s=0
    for i in range(0,N,k+1):
        s=s+l[i]
    return print(s)

N,k=map(int,input().split(","))
if N < 50 and k >= 0 and N > 0:
    prize(N,k)

#power of digits

def prize(N,k):
    l=[]
    for i in range(N):
        temp = int(input())
        if temp < 1000 and temp > 0:
            l.append(temp)
    s=0
    for i in range(0,N,k+1):
        s=s+l[i]
    return print(s)

N,k=map(int,input().split(","))
if N < 50 and k >= 0 and N > 0:
    prize(N,k)
