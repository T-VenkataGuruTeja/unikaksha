#24.grading system

a=int(input())
if a==100:
    print("S")
elif a<100 and a>90:
    print("A")
elif a<90 and a>80:
    print("B")
elif a<80 and a>70:
    print("C")
elif a<70 and a>60:
    print("D")
elif a<60 and a>50:
    print("E")
elif a<50:
    print("F")
else:
    print("Invalid Input")

#25.profit or loss

a=float(input())
b=int(input())
c=a-12*b
print("Enter the price of a dozen mangoes")
print("Enter the price at which 1 mango is being sold")
if c>0:
    print("LOSS : RS.%.2f"%(c))
elif c<0:
    print("PROFIT : RS.%.2f"%(12*b-a))
else:
    print("No profit nor loss")

