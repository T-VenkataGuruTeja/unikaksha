#60.largest number in a list

a=int(input())
l=[]
for i in range(a):
    temp = int(input())
    l.append(temp)
max = l[0]
for k in range(0,a):
    if max <= l[k]:
        max=l[k]
print(max)

#61.odd or even

a=int(input())
l=[]
o=[]
e=[]
for i in range(a):
    temp = int(input())
    l.append(temp)
for i in range(a):
    if l[i]%2 == 0:
        e.append(l[i])
    else:
        o.append(l[i])
print("The even list",e)
print("The odd list",o)

