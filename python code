#53.binary equivalent of a number using recursion

def binary(a,l):
    b=a%2
    l.append(b)
    a=a//2
    if a == 2:
        l.append(0)
        l.append(1)
        c=l[::-1]
        k=""
        for i in range(len(c)):
            k=k+str(c[i])
        return print(k)
    else:
        binary(a,l)
a=int(input())
l=[]
binary(a,l)

#54.fibonacci series

def fib(a,b,c,d):
    s=a+b
    a=b
    b=s
    print(s,end=" ")
    d=d+1
    if d==c:
        return
    else:
        fib(a,b,c,d)
c=int(input())
d=0
fib(-1,1,c,d)

#55.odd or even

def odd_or_even(a):
    if a==0:
        return print("even")
    elif a==1:
        return print("odd")
    else:
        odd_or_even(a-2)
a=int(input())
odd_or_even(a)

#56.factorial

def fact(a):
    if a==1 or a==0:
        return 1
    else:
        return a*fact(a-1)
a=int(input())
print(fact(a))

#57.lcm of two numbers

def lcm(a,b,c):
    if (c%a==0 and c%b==0):
        return print(c)
    elif c < a*b:
        lcm(a,b,c+1)
    else:
        return print(a*b)
a=int(input())
b=int(input())
lcm(a,b,1)

#58.decimal to binary

def binary(a,l):
    l.append(a%2)
    a=a//2
    if a == 2:
        l.append(0)
        l.append(1)
        c=l[::-1]
        k=""
        for i in c:
            k=k+str(i)
        return print(k)
    else:
        binary(a,l)
a=int(input())
l=[]
binary(a,l)

