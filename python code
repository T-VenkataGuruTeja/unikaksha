#63.sort the list

a=int(input())
l=[]
for i in range(a):
    temp = int(input())
    l.append(temp)
b=int(input())
for i in range(b):
    temp = int(input())
    l.append(temp)
for i in range(a+b):
    for k in range(a+b-1):
        if l[k] >= l[k+1]:
            l[k],l[k+1] = l[k+1],l[k]
print("Sorted list is: ",l)

#64.remove duplicates

a=int(input())
l=[]
c=[]
for i in range(a):
    temp = int(input())
    l.append(temp)
for i in l:
    if i not in c:
        c.append(i)
print(c)

#65.second largest number

a=int(input())
l=[]
for i in range(a):
    temp=int(input())
    l.append(temp)
for i in range(a):
    for k in range(a-1):
        if l[k]>=l[k+1]:
            l[k],l[k+1]=l[k+1],l[k]
print(l[-2])

#66.swap the number

a=int(input())
l=[]
for i in range(a):
    temp = int(input())
    l.append(temp)
for i in range(0,a//2,2):
    l[i],l[-i-1]=l[-i-1],l[i]
print(l)

