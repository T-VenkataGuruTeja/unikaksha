#36.amoeba multiplication

a=int(input())
s=0
b=0
c=1
print("Enter the number of months: ")
if a==1:
    print("The amoeba size is 0")
elif a==2:
    print("The amoeba size is 1")
else:
    for i in range(a-2):
        s=b+c
        b=c
        c=s
    print("The amoeba size is %d"%s)

#37.factorial

a=int(input())
s=1
