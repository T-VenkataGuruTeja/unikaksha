#80.characters in ascending order

a=input()
l=[]
for i in a:
    l.append(i)
for i in range(len(a)):
    for k in range(len(a)-1):
        if l[k]>l[k+1]:
            l[k],l[k+1]=l[k+1],l[k]
k=""
for i in range(len(a)):
    k=k+l[i]
print(k)#python

