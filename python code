#72.map a dictionary

a=int(input())
k=[]
v=[]
dist={}
for i in range(a):
    temp = int(input())
    k.append(temp)
for i in range(a):
    temp = int(input())
    v.append(temp)
for i in range(a):
    dist[k[i]] = v[i]
print("Enter number of elements for dictionary:")
print("For keys:")
print("For values:")
print("The dictionary is:")
print(dist)

