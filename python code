#83.replace all occurrences

a=input()
b=input()
c=input()
l=[]
for i in a:
    l.append(i)
d=l.index(b)
l.pop(d)
l.insert(d,c)
k=" "
for i in l:
    k=k+i
print("modified string: ")
print(k)

