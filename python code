#34.intersection of a circle

import math
a=int(input())
b=int(input())
c=int(input())
d=int(input())
e=int(input())
f=int(input())
g=math.sqrt(((a-d)**2) + ((b-e)**2))
if g == (c+f):
    print("The circles are tangential")
elif g > (c+f):
    print("The circles do not overlap")
else:
    print("The circles overlap")
